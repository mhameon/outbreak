{
  "name": "outbreak-server",
  "version": "1.0.0",
  "description": "Zombies experiments",
  "main": "src/index.ts",
  "scripts": {
    "start": "NODE_CONFIG_DIR=../config node build",
    "debug": "NODE_CONFIG_DIR=../config DEBUG=socket.io:* node build",
    "start:dev": "tsc-watch --onSuccess 'npm run start'",
    "build": "npm run clean && npm run create:symlink && echo 'Using TypeScript' && tsc --version && tsc",
    "lint": "eslint 'src/**'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "npm run build",
    "clean": "npx rimraf build",
    "create:symlink": "./symlink.sh"
  },
  "author": "Matt",
  "devDependencies": {
    "@types/config": "0.0.36",
    "@types/express": "^4.17.7",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.0",
    "@types/socket.io": "^2.1.11",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-mocha": "^7.0.1",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-plugin-typescript-sort-keys": "^1.3.0"
  },
  "dependencies": {
    "config": "^3.3.1",
    "express": "^4.17.1",
    "mocha": "^8.1.1",
    "module-alias": "^2.2.2",
    "socket.io": "^2.3.0",
    "spdy": "^4.0.2",
    "tsc-watch": "^4.2.9",
    "tumult": "^3.0.14",
    "typescript": "^3.9.7",
    "winston": "^3.3.3"
  },
  "_moduleAliases": {
    "@engine": "build/engine"
  }
}
